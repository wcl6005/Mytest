server {
    listen      80;
    server_name wuchunlong.eastasia.cloudapp.azure.com;
    root        /home/www/AccountOnline2-5b3794134d48;
    access_log  /home/www/AccountOnline2-5b3794134d48/logs/nginx.access.log;
    error_log   /home/www/AccountOnline2-5b3794134d48/logs/nginx.error.log;
    
    # 处理静态文件/favicon.ico:
    location /favicon.ico {
        root /home/www/AccountOnline2-5b3794134d48/src/mysite;
    }

    # 处理静态资源:
    location ~ ^\/static\/.*$ {
        root /home/www/AccountOnline2-5b3794134d48/src/mysite;
    }
    location ~ ^\/front\/.*$ {
        root /home/www/AccountOnline2-5b3794134d48/src;
    }
    location ~ ^\/example\/.*$ {
        root /home/www/AccountOnline2-5b3794134d48/src;
    }
    
    location / {
        proxy_pass       http://localhost:8000;
        proxy_set_header Host $host;
        proxy_set_header X-Real-IP $remote_addr;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
}
